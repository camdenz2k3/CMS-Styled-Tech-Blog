{{#each blogs}}
<div class="card my-2">
  <div class="card-header navbar-dark bg-secondary rounded d-flex justify-content-between align-items-center row">
    <a href="/editblog/{{id}}" class="col-8 col-sm-10 my-auto p-3 text-decoration-none text-reset">
      <h4>{{title}}</h4>
    </a>
  </div>
</div>
{{/each}}

<button id="grabPost" type="button" class="fs-4 py-2 px-3 text-light bg-primary rounded fixed-bottom w-25 mx-auto mb-2">Add New Post</button>

<script src="/js/dashboard.js"></script>